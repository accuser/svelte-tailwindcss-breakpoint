		"@typescript-eslint/parser": "^5.36.1",
		"autoprefixer": "^10.4.8",
		"cssnano": "^5.1.13",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.16",
		"postcss-nesting": "^10.1.10",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.9.0",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.16",
		"tailwindcss": "^3.1.8",
		"tslib": "^2.4.0",
		"typescript": "^4.8.2",
		"vite": "3.1.0-beta.1"
	},
	"type": "module"
}
