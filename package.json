{
	"name": "@accuser/breakpoint",
	"version": "0.2.0",
	"description": "A simple breakpoint utility for SvelteKit and TailwindCSS",
	"keywords": [
		"svelte",
		"sveltekit",
		"tailwindcss"
	],
	"homepage": "https://github.com/accuser/svelte-tailwindcss-breakpoint/README.md",
	"bugs": {
		"url": "https://github.com/accuser/svelte-tailwindcss-breakpoint/issues"
	},
	"license": "MIT",
	"author": {
		"name": "Matthew Gibbons",
		"url": "https://github.com/accuser"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/accuser/svelte-tailwindcss-breakpoint.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "1.0.0-next.70",
		"@sveltejs/kit": "1.0.0-next.459",
